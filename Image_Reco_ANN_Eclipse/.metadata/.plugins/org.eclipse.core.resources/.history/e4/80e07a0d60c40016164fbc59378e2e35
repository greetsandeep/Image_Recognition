package artificialneuralnetworks;

public class SunGlasses {
	int correct = 0;
	int wrong = 0;
	int prediction = 0;
	
	public void updateResults(int prediction,String filename){
		if(prediction == 1)
		{
			if(filename.contains("sunglasses"))
				correct++;
			else
				wrong++;
		}
		else{
			if(filename.contains("open"))
				correct++;
			else
				wrong++;
		}
	}
}
