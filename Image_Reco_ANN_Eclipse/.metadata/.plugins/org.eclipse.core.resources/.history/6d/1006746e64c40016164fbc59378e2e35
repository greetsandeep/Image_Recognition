package artificialneuralnetworks;

public class FaceRecognizer {
	int correct = 0;
	int wrong = 0;
	int prediction = 0;

	
	public void updateResults(int prediction,String filename){
		switch (prediction) {
		case 1:
			if(filename.contains("an2i"))
				correct++;
			else
				wrong++;
			break;
		case 2:
			if(filename.contains("at33"))
				correct++;
			else
				wrong++;
			break;
		case 3:
			if(filename.contains("boland"))
				correct++;
			else
				wrong++;
			break;
		case 4:
			if(filename.contains("bpm"))
				correct++;
			else
				wrong++;
			break;
		case 5:
			if(filename.contains("ch4f"))
				correct++;
			else
				wrong++;
			break;
		case 6:
			if(filename.contains("cheyer"))
				correct++;
			else
				wrong++;
			break;
		case 7:
			if(filename.contains("choon"))
				correct++;
			else
				wrong++;
			break;
		case 8:
			if(filename.contains("danieln"))
				correct++;
			else
				wrong++;
			break;
		case 9:
			if(filename.contains("glickman"))
				correct++;
			else
				wrong++;
			break;
		case 10:
			if(filename.contains("karyadi"))
				correct++;
			else
				wrong++;
			break;
		case 11:
			if(filename.contains("kawamura"))
				correct++;
			else
				wrong++;
			break;
		case 12:
			if(filename.contains("kk49"))
				correct++;
			else
				wrong++;
			break;
		case 13:
			if(filename.contains("megak"))
				correct++;
			else
				wrong++;
			break;

		default:
			break;
		}
	}
}
